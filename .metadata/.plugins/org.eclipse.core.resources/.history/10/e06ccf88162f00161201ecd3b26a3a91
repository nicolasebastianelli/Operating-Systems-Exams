	if (argc != 4) {
			fprintf(stderr, "Numero di argomenti errato\n");
			exit(EXIT_FAILURE);
		}
	if (argc != 4) {
				fprintf(stderr, "Numero di argomenti errato\n");
				exit(EXIT_FAILURE);
			}

}
